    <term_definitions language="en">
      <items code="at0000">
        <items id="text">Heart Rate</items>
        <items id="description">Measurement of the heart rate and description of associated characteristics.</items>
      </items>
      <items code="at0001">
        <items id="text">structure</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0002">
        <items id="text">history</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0003">
        <items id="text">Any event</items>
        <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
      </items>
      <items code="at0004">
        <items id="text">Heart Rate</items>
        <items id="description">The rate of the heart in beats per minute.</items>
      </items>
      <items code="at0005">
        <items id="text">Regularity</items>
        <items id="description">The observed regularity of the heart rate.</items>
      </items>
      <items code="at0006">
        <items id="text">Regular</items>
        <items id="description">Heart rhythm is regular.</items>
      </items>
      <items code="at0007">
        <items id="text">Regularly irregular</items>
        <items id="description">Heart rhythm is regularly irregular.</items>
      </items>
      <items code="at0008">
        <items id="text">Irregularly irregular</items>
        <items id="description">Heart rhythm is irregular in a chaotic manner.</items>
      </items>
      <items code="at0009">
        <items id="text">Comment</items>
        <items id="description">Comment about the heart rate.</items>
      </items>
      <items code="at0010">
        <items id="text">List</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0012">
        <items id="text">List</items>
        <items id="description">@ internal @</items>
      </items>
      <items code="at0013">
        <items id="text">Position</items>
        <items id="description">The position of the patient when the heartbeat was recorded.</items>
      </items>
      <items code="at1000">
        <items id="text">Lying</items>
        <items id="description">Lying flat at the time of heart rate measurement.</items>
      </items>
      <items code="at1001">
        <items id="text">Sitting</items>
        <items id="description">Sitting (for example on bed or chair) at the time of heart rate measurement.</items>
      </items>
      <items code="at1002">
        <items id="text">Reclining</items>
        <items id="description">Reclining at the time of heart rate measurement.</items>
      </items>
      <items code="at1003">
        <items id="text">Standing</items>
        <items id="description">Standing at the time of heart rate measurement.</items>
      </items>
      <items code="at1005">
        <items id="text">Heart Beat Present?</items>
        <items id="description">Is a heart beat present?</items>
        <items id="comment">The heart beat is present is implied as true if rate &gt;0.</items>
      </items>
      <items code="at1013">
        <items id="text">Device</items>
        <items id="description">Details about the device used to observe the heart rate and rhythm.</items>
      </items>
      <items code="at1017">
        <items id="text">Exertion</items>
        <items id="description">Details about physical exertion being undertaken at the time of recording heart rate and/or rhythm.</items>
      </items>
      <items code="at1018">
        <items id="text">Confounding Factors</items>
        <items id="description">Narrative description about any incidental factors that may be contributing to the heart rate observations.</items>
        <items id="comment">For example, presence of a pacemaker, level of anxiety; pain or fever etc.</items>
      </items>
      <items code="at1019">
        <items id="text">Method</items>
        <items id="description">Method used to measure the heart rate.</items>
        <items id="comment">For example, auscultation or electronic monitoring.</items>
      </items>
      <items code="at1022">
        <items id="text">Clinical Description</items>
        <items id="description">Narrative description of the heart rate.</items>
      </items>
      <items code="at1023">
        <items id="text">Clinical Interpretation</items>
        <items id="description">Single word, phrase or brief description representing the heart rate findings.</items>
        <items id="comment">Coding with a terminology is preferred, where possible. Examples include: Bradycardia, Extrasystoles or Sinus rhythm. Multiple statements are allowed.</items>
      </items>
    </term_definitions>
